 RowBox[{
  RowBox[{
   RowBox[{"ACRejillaCuadrada", "[", 
    RowBox[{
    "configuracionInicial_", ",", " ", "reglaEntrada_", ",", " ", "iters_"}], 
    "]"}], " ", ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "configuracion", ",", " ", "listaRegla", ",", " ", "res", ",", " ", 
       "rejilla", ",", " ", "salida", ",", " ", "size", ",", " ", "i", ",", 
       " ", "k"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"configuracion", "=", "configuracionInicial"}], ";", 
