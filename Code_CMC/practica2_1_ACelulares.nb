      "\[IndentingNewLine]", 
      RowBox[{"resList", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"j", "=", "0"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", " ", 
       RowBox[{
        RowBox[{"j", "<", "8"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"auxList", "=", 
          RowBox[{"IntegerDigits", "[", 
           RowBox[{"j", ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"j", "<", "2"}], ",", " ", 
           RowBox[{"auxList", " ", "=", " ", 
            RowBox[{"Join", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"0", ",", "0"}], "}"}], ",", " ", "auxList"}], "]"}]}],
            ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"If", "[", 
