Cell[BoxData[
 RowBox[{
  RowBox[{"JuegoDeLaVida", "[", 
   RowBox[{"rejilla_", ",", " ", "pasos_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "imagen", ",", " ", "secuencia", ",", " ", "numFilas", ",", " ", 
      "numColumnas", ",", " ", "siguienteImagen", ",", " ", "fila", ",", " ", 
      "filaActualizada", ",", " ", "counter", ",", " ", "limArriba", ",", " ",
       "limAbajo", ",", " ", "limIzquierda", ",", " ", "limDerecha", ",", " ",
       "salida", ",", " ", "i", ",", " ", "j", ",", " ", "k", ",", " ", "n"}],
      "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"imagen", "=", "rejilla"}], ";", "\[IndentingNewLine]", 
     RowBox[{"secuencia", "=", 
      RowBox[{"{", "imagen", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"numFilas", "=", 
      RowBox[{"Length", "[", "imagen", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"numColumnas", " ", "=", 
      RowBox[{"Length", "[", 
       RowBox[{"imagen", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", " ", 
       RowBox[{"i", "<=", "pasos"}], ",", " ", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"siguienteImagen", "=", 
         RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"j", "=", "1"}], ",", " ", 
          RowBox[{"j", "<=", " ", "numFilas"}], ",", " ", 
          RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"fila", "=", 
            RowBox[{"imagen", "[", 
             RowBox[{"[", "j", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"filaActualizada", "=", 
            RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"k", "=", "1"}], ",", " ", 
             RowBox[{"k", "<=", 
              RowBox[{"Length", "[", "fila", "]"}]}], ",", " ", 
             RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"counter", "=", "0"}], ";", "\[IndentingNewLine]", 
              RowBox[{"limArriba", "=", 
               RowBox[{"j", "-", "1"}]}], ";", "\[IndentingNewLine]", 
              RowBox[{"limAbajo", "=", 
               RowBox[{"j", "+", "1"}]}], ";", "\[IndentingNewLine]", 
              RowBox[{"limIzquierda", "=", 
               RowBox[{"k", "-", "1"}]}], ";", "\[IndentingNewLine]", 
              RowBox[{"limDerecha", "=", 
               RowBox[{"k", "+", "1"}]}], ";", "\[IndentingNewLine]", 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"j", "==", "1"}], ",", " ", 
                RowBox[{"limArriba", "=", "numFilas"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "si", " ", "est\[AAcute]", " ", "en", " ", 
                "el", " ", "borde", " ", "superior"}], "*)"}], 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"j", "==", "numFilas"}], ",", " ", 
                RowBox[{"limAbajo", "=", "1"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "si", " ", "est\[AAcute]", " ", "en", " ", 
                "el", " ", "borde", " ", "inferior"}], "*)"}], 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"k", "==", "1"}], ",", " ", 
                RowBox[{"limIzquierda", "=", "numColumnas"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "si", " ", "est\[AAcute]", " ", "en", " ", 
                "el", " ", "borde", " ", "de", " ", "la", " ", "izquierda"}], 
               "*)"}], "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"k", "==", "numColumnas"}], ",", " ", 
                RowBox[{"limDerecha", "=", "1"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "si", " ", "est\[AAcute]", " ", "en", " ", 
                "el", " ", "borde", " ", "de", " ", "la", " ", "derecha"}], 
               "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"imagen", "[", 
                  RowBox[{"[", 
                   RowBox[{"limArriba", ",", "limIzquierda"}], "]"}], "]"}], "==",
                  "1"}], ",", " ", 
                RowBox[{"counter", "++"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "la", " ", "c\[EAcute]lula", " ", "de", " ", 
                "arriba", " ", "a", " ", "la", " ", "izquierda"}], "*)"}], 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"imagen", "[", 
                  RowBox[{"[", 
                   RowBox[{"limArriba", ",", "k"}], "]"}], "]"}], "==", "1"}],
                 ",", " ", 
                RowBox[{"counter", "++"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "la", " ", "c\[EAcute]lula", " ", "de", " ", 
                "arriba"}], "*)"}], "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"imagen", "[", 
                  RowBox[{"[", 
                   RowBox[{"limArriba", ",", "limDerecha"}], "]"}], "]"}], "==",
                  "1"}], ",", " ", 
                RowBox[{"counter", "++"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "la", " ", "c\[EAcute]lula", " ", "de", " ", 
                "arriba", " ", "a", " ", "la", " ", "derecha"}], "*)"}], 
              "\[IndentingNewLine]", "\[IndentingNewLine]", 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"imagen", "[", 
                  RowBox[{"[", 
                   RowBox[{"j", ",", "limIzquierda"}], "]"}], "]"}], "==", 
                 "1"}], ",", " ", 
                RowBox[{"counter", "++"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "la", " ", "c\[EAcute]lula", " ", "de", " ", 
                "la", " ", "izquierda"}], "*)"}], "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"imagen", "[", 
                  RowBox[{"[", 
                   RowBox[{"j", ",", "limDerecha"}], "]"}], "]"}], "==", 
                 "1"}], ",", " ", 
                RowBox[{"counter", "++"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "la", " ", "c\[EAcute]lula", " ", "de", " ", 
                "la", " ", "derecha"}], "*)"}], "\[IndentingNewLine]", 
              "\[IndentingNewLine]", "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"imagen", "[", 
                  RowBox[{"[", 
                   RowBox[{"limAbajo", ",", "limIzquierda"}], "]"}], "]"}], "==",
                  "1"}], ",", " ", 
                RowBox[{"counter", "++"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "la", " ", "c\[EAcute]lula", " ", "de", " ", 
                "abajo", " ", "a", " ", "la", " ", "izquierda"}], "*)"}], 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"imagen", "[", 
                  RowBox[{"[", 
                   RowBox[{"limAbajo", ",", "k"}], "]"}], "]"}], "==", "1"}], 
                ",", " ", 
                RowBox[{"counter", "++"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "la", " ", "c\[EAcute]lula", " ", "de", " ", 
                "abajo"}], "*)"}], "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"imagen", "[", 
                  RowBox[{"[", 
                   RowBox[{"limAbajo", ",", "limDerecha"}], "]"}], "]"}], "==",
                  "1"}], ",", " ", 
                RowBox[{"counter", "++"}]}], "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{
               "Comprueba", " ", "la", " ", "c\[EAcute]lula", " ", "de", " ", 
                "abajo", " ", "a", " ", "la", " ", "derecha"}], "*)"}], 
              "\[IndentingNewLine]", "\[IndentingNewLine]", 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"imagen", "[", 
                   RowBox[{"[", 
                    RowBox[{"j", ",", "k"}], "]"}], "]"}], "==", "1"}], ")"}],
                 ",", "\[IndentingNewLine]", 
                RowBox[{"If", "[", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"counter", "==", "2"}], " ", "||", " ", 
                    RowBox[{"counter", "==", "3"}]}], ")"}], ",", 
                  "\[IndentingNewLine]", 
                  RowBox[{"AppendTo", "[", 
                   RowBox[{"filaActualizada", ",", " ", "1"}], "]"}], ",", 
                  "\[IndentingNewLine]", 
                  RowBox[{"AppendTo", "[", 
                   RowBox[{"filaActualizada", ",", " ", "0"}], "]"}]}], 
                 "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"If", "[", 
                  RowBox[{
                   RowBox[{"(", 
                    RowBox[{"counter", "==", "3"}], ")"}], ",", 
                   "\[IndentingNewLine]", 
                   RowBox[{"AppendTo", "[", 
                    RowBox[{"filaActualizada", ",", " ", "1"}], "]"}], ",", 
                   "\[IndentingNewLine]", 
                   RowBox[{"AppendTo", "[", 
                    RowBox[{"filaActualizada", ",", " ", "0"}], "]"}]}], 
                  "\[IndentingNewLine]", "]"}], ";"}]}], 
               RowBox[{"(*", 
                RowBox[{
                "If", " ", "que", " ", "comprueba", " ", "si", " ", "la", " ",
                  "c\[EAcute]lula", " ", "se", " ", "mantiene", " ", "viva", 
                 " ", 
                 RowBox[{"(", 
                  RowBox[{"o", " ", "muerta"}], ")"}], " ", "o", " ", "si", 
                 " ", "cambia", " ", "de", " ", "estado"}], "*)"}], 
               "\[IndentingNewLine]", "]"}], ";"}]}], 
            RowBox[{"(*", 
             RowBox[{
             "If", " ", "que", " ", "comprueba", " ", "si", " ", "la", " ", 
              "c\[EAcute]lula", " ", "est\[AAcute]", " ", "viva"}], "*)"}], 
            "\[IndentingNewLine]", "]"}], ";", 
           RowBox[{"(*", 
            RowBox[{"For", " ", "k"}], "*)"}], "\[IndentingNewLine]", 
           RowBox[{"AppendTo", "[", 
            RowBox[{"siguienteImagen", ",", " ", "filaActualizada"}], "]"}], 
           ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
        RowBox[{"(*", 
         RowBox[{"For", " ", "j"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"secuencia", ",", " ", "siguienteImagen"}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"imagen", "=", "siguienteImagen"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", 
     RowBox[{"(*", 
      RowBox[{"For", " ", "i"}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"salida", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"n", "=", "1"}], ",", 
       RowBox[{"n", "<=", 
        RowBox[{"Length", "[", "secuencia", "]"}]}], ",", 
       RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"AppendTo", "[", 
         RowBox[{"salida", ",", 
          RowBox[{"ArrayPlot", "[", 
           RowBox[{"secuencia", "[", 
            RowBox[{"[", "n", "]"}], "]"}], "]"}]}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", 
     RowBox[{"(*", 
      RowBox[{"For", " ", "n"}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"ListAnimate", "[", "salida", "]"}], "]"}], ";"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
