 RowBox[{
  RowBox[{
   RowBox[{"obtenerRegla", "[", "n_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "list", ",", " ", "extraZeroes", ",", " ", "resList", ",", " ", 
       "auxList", ",", " ", "i", ",", "j"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"list", "=", 
       RowBox[{"IntegerDigits", "[", 
        RowBox[{"n", ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"extraZeroes", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
        RowBox[{"i", "<=", 
         RowBox[{"(", 
          RowBox[{"8", "-", 
           RowBox[{"Length", "[", "list", "]"}]}], ")"}]}], ",", " ", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"AppendTo", "[", 
          RowBox[{"extraZeroes", ",", " ", "0"}], "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", " ", 
