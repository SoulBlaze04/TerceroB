      "\[IndentingNewLine]", 
      RowBox[{"size", " ", "=", " ", 
       RowBox[{"Length", "[", "configuracion", "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"k", "=", "1"}], ",", " ", 
        RowBox[{"k", "<=", "iters"}], ",", " ", 
        RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"configuracion", " ", "=", " ", 
          RowBox[{"siguienteIteracion", "[", 
           RowBox[{"configuracion", ",", " ", "listaRegla"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Length", "[", "res", "]"}], " ", "<", " ", "size"}], ",",
            "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"res", " ", "=", " ", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"res", ",", " ", "configuracion"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"rejilla", " ", "=", " ", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"rejilla", ",", " ", "res"}], "]"}]}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"res", " ", "=", " ", 
             RowBox[{"Drop", "[", 
              RowBox[{"res", ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"res", " ", "=", " ", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"res", ",", " ", "configuracion"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"rejilla", " ", "=", " ", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"rejilla", ",", " ", "res"}], "]"}]}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";"}]}], " ", 
       RowBox[{"(*", 
        RowBox[{
        "If", " ", "que", " ", "comprueba", " ", "si", " ", "se", " ", 
         "supera", " ", "el", " ", "tama\[NTilde]o", " ", "de", " ", "la", 
         " ", "rejilla"}], "*)"}], "\[IndentingNewLine]", "]"}], ";", " ", 
      RowBox[{"(*", 
       RowBox[{"For", " ", "k"}], "*)"}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"salida", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
