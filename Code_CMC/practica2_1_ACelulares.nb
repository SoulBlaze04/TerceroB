              RowBox[{"j", "<", "4"}], ",", " ", 
              RowBox[{"auxList", " ", "=", " ", 
               RowBox[{"Join", "[", 
                RowBox[{
                 RowBox[{"{", "0", "}"}], ",", " ", "auxList"}], "]"}]}]}], 
             "]"}], ";"}]}], " ", 
          RowBox[{"(*", 
           RowBox[{
            RowBox[{"If", " ", "j"}], " ", "<", " ", "4"}], "*)"}], 
          "\[IndentingNewLine]", "]"}], ";", " ", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{"If", " ", "j"}], " ", "<", " ", "2"}], "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{"resList", " ", "=", " ", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"resList", ",", " ", 
            RowBox[{"AppendTo", "[", 
             RowBox[{"auxList", ",", " ", 
              RowBox[{"list", "[", 
               RowBox[{"[", 
                RowBox[{"j", "+", "1"}], "]"}], "]"}]}], "]"}]}], "]"}]}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{"j", "++"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", " ", 
      RowBox[{"(*", 
       RowBox[{"While", " ", "j"}], "*)"}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "resList", "]"}], ";"}]}], "\[IndentingNewLine]",
     "]"}]}], ";"}]], "Input",
