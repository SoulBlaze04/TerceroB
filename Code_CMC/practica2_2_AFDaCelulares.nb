Cell[BoxData[
 RowBox[{
  RowBox[{"dibujaAutomata", "[", 
   RowBox[{"cadena_", ",", " ", "AC_", ",", "frontera_"}], " ", "]"}], " ", ":=",
   " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "chain", ",", " ", "prevChain", ",", " ", "nextChain", ",", " ", 
      "simbolos", ",", " ", "f", ",", " ", "finales", ",", " ", "m", ",", " ",
       "n", ",", " ", "i", ",", " ", "j", ",", " ", "k", ",", " ", 
      "vecinoIzq", ",", " ", "celula", ",", " ", "transicion", ",", " ", 
      "res", ",", " ", "secuencia", ",", " ", "imagen", ",", "salida"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"prevChain", " ", "=", " ", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"chain", " ", "=", " ", "cadena"}], ";", "\[IndentingNewLine]", 
     RowBox[{"simbolos", " ", "=", " ", 
      RowBox[{"AC", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"f", " ", "=", " ", 
      RowBox[{"AC", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"finales", " ", "=", " ", 
      RowBox[{"AC", "[", 
       RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"imagen", "=", 
      RowBox[{"{", "chain", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"secuencia", "=", 
      RowBox[{"{", "imagen", "}"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", " ", 
       RowBox[{"i", "<=", 
        RowBox[{"Length", "[", "chain", "]"}]}], ",", " ", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"!", 
           RowBox[{"MemberQ", "[", 
            RowBox[{"simbolos", ",", " ", 
             RowBox[{"chain", "[", 
              RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Return", "[", "False", "]"}], ";"}]}], 
         "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"n", "=", "1"}], ",", " ", 
       RowBox[{"n", "<=", 
        RowBox[{"Length", "[", "cadena", "]"}]}], ",", " ", 
       RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"vecinoIzq", " ", "=", " ", "frontera"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"nextChain", " ", "=", " ", 
         RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"j", "=", "1"}], ",", " ", 
          RowBox[{"j", "<=", 
           RowBox[{"Length", "[", "chain", "]"}]}], ",", " ", 
          RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"celula", " ", "=", " ", 
            RowBox[{"chain", "[", 
             RowBox[{"[", "j", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Length", "[", 
               RowBox[{"Cases", "[", 
                RowBox[{"f", ",", " ", 
                 RowBox[{"{", 
                  RowBox[{"vecinoIzq", ",", " ", "celula", ",", " ", "_"}], 
                  "}"}]}], "]"}], "]"}], "==", "0"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"Return", "[", "False", "]"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"transicion", " ", "=", 
               RowBox[{"Flatten", "[", 
                RowBox[{"Cases", "[", 
                 RowBox[{"f", ",", " ", 
                  RowBox[{"{", 
                   RowBox[{"vecinoIzq", ",", " ", "celula", ",", " ", "_"}], 
                   "}"}]}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
              RowBox[{"AppendTo", "[", 
               RowBox[{"nextChain", ",", " ", 
                RowBox[{"transicion", "[", 
                 RowBox[{"[", "3", "]"}], "]"}]}], "]"}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"vecinoIzq", " ", "=", " ", "celula"}], ";"}]}], 
            "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
         "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"prevChain", " ", "=", " ", "chain"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"chain", " ", "=", " ", "nextChain"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"imagen", ",", " ", "chain"}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"AppendTo", "[", 
         RowBox[{"secuencia", ",", " ", "imagen"}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"res", " ", "=", " ", "False"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"k", "=", "1"}], ",", " ", 
       RowBox[{"k", "<=", 
        RowBox[{"Length", "[", "chain", "]"}]}], ",", " ", 
       RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"MemberQ", "[", 
           RowBox[{"finales", ",", " ", 
            RowBox[{"chain", "[", 
             RowBox[{"[", "k", "]"}], "]"}]}], "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"res", " ", "=", " ", "True"}], ";"}]}], 
         "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"salida", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"m", "=", "1"}], ",", 
       RowBox[{"m", "<=", 
        RowBox[{"Length", "[", "secuencia", "]"}]}], ",", 
       RowBox[{"m", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"AppendTo", "[", 
         RowBox[{"salida", ",", 
          RowBox[{"ArrayPlot", "[", 
           RowBox[{
            RowBox[{"secuencia", "[", 
             RowBox[{"[", "m", "]"}], "]"}], ",", 
            RowBox[{"ColorRules", "->", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"q", "->", "Yellow"}], ",", 
               RowBox[{"p", "->", "Purple"}], ",", " ", 
               RowBox[{"r", "->", "LightGreen"}], ",", " ", 
               RowBox[{"a", "->", "Red"}], ",", " ", 
               RowBox[{"b", "->", "Blue"}], ",", " ", 
               RowBox[{"0", "->", "Green"}], ",", " ", 
               RowBox[{"1", "->", "Pink"}], ",", " ", 
               RowBox[{"2", "->", "White"}], ",", " ", 
               RowBox[{"3", "->", "Gray"}], ",", " ", 
               RowBox[{"4", "->", "LightBlue"}], ",", " ", 
               RowBox[{"5", "->", "LightPurple"}]}], "}"}]}], ",", " ", 
            RowBox[{"Mesh", "->", "True"}], ",", " ", 
            RowBox[{"MeshStyle", "\[Rule]", "Black"}]}], "]"}]}], "]"}], 
        ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
     RowBox[{"(*", 
      RowBox[{"For", " ", "n"}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"ListAnimate", "[", "salida", "]"}], "]"}], ";"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
