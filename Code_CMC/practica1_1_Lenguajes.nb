Cell[BoxData[
 RowBox[{
  RowBox[{"DirectamenteNoGenerativo", "[", "G_", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "g", ",", " ", "auxiliares", ",", " ", "terminales", ",", " ", 
      "producciones", ",", " ", "axioma", ",", " ", "res", ",", " ", "i", ",",
       " ", "j", ",", " ", "k", ",", " ", "sim", ",", " ", "prod", ",", " ", 
      "bool", ",", " ", "aux", ",", " ", "simbool", ",", " ", "term"}], "}"}],
     ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"g", " ", "=", " ", "G"}], ";", "\[IndentingNewLine]", 
     RowBox[{"auxiliares", " ", "=", " ", 
      RowBox[{"g", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"terminales", " ", "=", " ", 
      RowBox[{"g", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"producciones", " ", "=", " ", 
      RowBox[{"g", "[", 
       RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"axioma", " ", "=", " ", 
      RowBox[{"g", "[", 
       RowBox[{"[", "4", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"res", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", " ", 
       RowBox[{"i", "<=", 
        RowBox[{"Length", "[", "producciones", "]"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"sim", " ", "=", " ", 
         RowBox[{"producciones", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "1"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"prod", " ", "=", " ", 
         RowBox[{"producciones", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "2"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"bool", " ", "=", " ", "True"}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{"bool", ",", " ", 
          RowBox[{
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"j", " ", "=", " ", "1"}], ",", " ", 
             RowBox[{"j", "<=", 
              RowBox[{"Length", "[", "prod", "]"}]}], ",", 
             RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"aux", "=", 
               RowBox[{"prod", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
              RowBox[{"simbool", "=", "False"}], ";", "\[IndentingNewLine]", 
              RowBox[{"For", "[", 
               RowBox[{
                RowBox[{"k", "=", "1"}], ",", " ", 
                RowBox[{"k", "<=", 
                 RowBox[{"Length", "[", "aux", "]"}]}], ",", 
                RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"term", "=", 
                  RowBox[{"aux", "[", 
                   RowBox[{"[", "k", "]"}], "]"}]}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"If", "[", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"MemberQ", "[", 
                    RowBox[{"auxiliares", ",", " ", "term"}], "]"}], " ", "&&",
                     " ", 
                    RowBox[{"(", 
                    RowBox[{"term", " ", "==", 
                    RowBox[{"sim", "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}], ")"}]}], ",", " ", 
                   RowBox[{"simbool", "=", "True"}]}], "]"}], ";"}]}], 
               "\[IndentingNewLine]", "]"}], ";", 
              RowBox[{"(*", 
               RowBox[{"For", " ", "k"}], "*)"}], "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"!", "simbool"}], ",", " ", 
                RowBox[{"bool", "=", "False"}]}], "]"}], ";"}]}], 
            "\[IndentingNewLine]", "]"}], ";", 
           RowBox[{"(*", 
            RowBox[{"For", " ", "j"}], "*)"}], "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"bool", " ", "&&", " ", 
              RowBox[{"(", 
               RowBox[{"!", 
                RowBox[{"MemberQ", "[", 
                 RowBox[{"res", ",", " ", "sim"}], "]"}]}], ")"}]}], " ", ",",
              " ", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"res", ",", " ", "sim"}], "]"}]}], "]"}], ";"}]}], 
         "\[IndentingNewLine]", "]"}], ";"}]}], 
      RowBox[{"(*", 
