Cell[BoxData[
 RowBox[{
  RowBox[{"Greibach", "[", "G_", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "g", ",", " ", "auxiliares", ",", " ", "terminales", ",", " ", 
      "producciones", ",", " ", "axioma", ",", " ", "bool", ",", "i", ",", 
      " ", "j", ",", " ", "k", ",", " ", "sim", ",", " ", "prod", ",", " ", 
      "aux", ",", " ", "term"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"g", " ", "=", " ", "G"}], ";", "\[IndentingNewLine]", 
     RowBox[{"auxiliares", " ", "=", " ", 
      RowBox[{"g", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"terminales", " ", "=", " ", 
      RowBox[{"g", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"producciones", " ", "=", " ", 
      RowBox[{"g", "[", 
       RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"axioma", " ", "=", " ", 
      RowBox[{"g", "[", 
       RowBox[{"[", "4", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"bool", " ", "=", " ", "True"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", " ", 
       RowBox[{"i", "<=", 
        RowBox[{"Length", "[", "producciones", "]"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"sim", "=", 
         RowBox[{"producciones", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "1"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Length", "[", "sim", "]"}], ">", "1"}], ")"}], "||", 
           RowBox[{"(", 
            RowBox[{"!", 
             RowBox[{"MemberQ", "[", 
              RowBox[{"auxiliares", ",", 
               RowBox[{"sim", "[", 
                RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ")"}]}], ",", 
          RowBox[{"bool", "=", "False"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"prod", "=", 
            RowBox[{"producciones", "[", 
             RowBox[{"[", 
              RowBox[{"i", ",", "2"}], "]"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"j", "=", "1"}], ",", " ", 
             RowBox[{"j", "<=", 
              RowBox[{"Length", "[", "prod", "]"}]}], ",", 
             RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"aux", "=", 
               RowBox[{"prod", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"aux", "==", 
                 RowBox[{"{", "}"}]}], ",", " ", 
                RowBox[{"bool", "=", "False"}], ",", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"If", "[", 
                  RowBox[{
                   RowBox[{"!", 
                    RowBox[{"MemberQ", "[", 
                    RowBox[{"terminales", ",", " ", 
                    RowBox[{"aux", "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ",", " ", 
                   RowBox[{"bool", "=", "False"}], ",", "\[IndentingNewLine]", 
                   RowBox[{
                    RowBox[{"For", "[", 
                    RowBox[{
                    RowBox[{"k", "=", "2"}], ",", " ", 
                    RowBox[{"k", "<=", 
                    RowBox[{"Length", "[", "aux", "]"}]}], ",", " ", 
                    RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
                    RowBox[{
                    RowBox[{"term", "=", 
                    RowBox[{"aux", "[", 
                    RowBox[{"[", "k", "]"}], "]"}]}], ";", 
                    "\[IndentingNewLine]", 
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{"!", 
                    RowBox[{"MemberQ", "[", 
                    RowBox[{"auxiliares", ",", " ", "term"}], "]"}]}], ",", 
                    " ", 
                    RowBox[{"bool", "=", "False"}]}], "]"}], ";"}]}], 
                    "\[IndentingNewLine]", "]"}], ";"}]}], 
                  RowBox[{"(*", 
                   RowBox[{"For", " ", "k"}], "*)"}], "\[IndentingNewLine]", 
                  "]"}], ";"}]}], 
               RowBox[{"(*", 
                RowBox[{"If", " ", 
                 RowBox[{"aux", "[", 
                  RowBox[{"[", "i", "]"}], "]"}], " ", "en", " ", 
                 "terminales"}], "*)"}], "\[IndentingNewLine]", "]"}], 
              ";"}]}], 
            RowBox[{"(*", 
             RowBox[{"If", " ", "aux", " ", "es", " ", "lambda"}], "*)"}], 
            "\[IndentingNewLine]", "]"}], ";"}]}], 
         RowBox[{"(*", 
          RowBox[{"For", " ", "j"}], "*)"}], "\[IndentingNewLine]", "]"}], 
        ";"}]}], 
      RowBox[{"(*", 
       RowBox[{
       "If", " ", "el", " ", "antecedente", " ", "es", " ", 
        "v\[AAcute]lido"}], "*)"}], "\[IndentingNewLine]", "]"}], ";", 
     RowBox[{"(*", 
      RowBox[{"For", " ", "i"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Return", " ", "[", "bool", "]"}], ";"}]}], 
   "\[IndentingNewLine]", "]"}], 
  RowBox[{"(*", "Module", "*)"}]}]], "Input",
 CellChangeTimes->{{3.9180471042843266`*^9, 3.918047104867228*^9}, {
